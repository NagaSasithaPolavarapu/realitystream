    },
    {
      "cell_type": "code",
      "source": [
        "!pip install \"datacommons-client[Pandas]\" --upgrade --quiet\n",
        "\n",
        "import pandas as pd\n",
        "from datacommons_client import DataCommonsClient"
      ],
